{
  "name": "longaodai/laravel-repository",
  "description": "A clean and simple repository pattern implementation for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vo Chi Long",
      "email": "vochilong.work@gmail.com"
    }
  ],
  "keywords": [
    "laravel",
    "repository",
    "repository-pattern",
    "service-layer"
  ],
  "require": {
    "php": ">=8.0",
    "illuminate/support": ">=9.0"
  },
  "autoload": {
    "psr-4": {
      "LongAoDai\\Repository\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LongAoDai\\Repository\\RepositoryServiceProvider"
      ]
    }
  },
  "support": {
    "issues": "https://github.com/longaodai/laravel-repository/issues",
    "source": "https://github.com/longaodai/laravel-repository"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
